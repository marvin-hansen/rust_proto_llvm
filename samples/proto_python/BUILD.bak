# compile proto libraries to python
load("//.build/bazel/proto:defs.bzl", "fly_proto")

fly_proto(
    name = "sample_proto",
    srcs = [
        "sample.proto",
    ],
    deps = [
        "@com_google_protobuf//:duration_proto",
        "@com_google_protobuf//:timestamp_proto",
    ],
)

# --- GLOBAL ---

fly_proto(
    name = "python_proto_bindings",
    deps = [
        ":sample_proto",
    ],
     visibility = ["//visibility:public"],
)
